@namespace PrimeBlazor

<div class="p-toolbar p-component @CssClass" role="toolbar">
    <div class="p-toolbar-group-left">
        @StartContentTemplate
    </div>
    <div class="p-toolbar-group-right">
        @EndContentTemplate
    </div>
</div>

@code {

    [Parameter]
    public string Style { get; set; }

    [Parameter]
    public string CssClass { get; set; }

    [Parameter]
    public RenderFragment StartContentTemplate { get; set; }

    [Parameter]
    public RenderFragment EndContentTemplate { get; set; }
}